<Window x:Class="WfDesignerWpf.SetDocumentPropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WfDesignerWpf"
        mc:Ignorable="d"
        Title="SetDocumentPropertiesWindow" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0">
            <Menu Height="25" VerticalAlignment="Top">
                <MenuItem Header="File">
                    <MenuItem Header="Save" Click="Save"/>
                    <MenuItem Header="Close" Click="Close"/>
                </MenuItem>
            </Menu>
        </StackPanel>
        <StackPanel Name="StackPanel" Grid.Row="1"/>
    </Grid>
</Window>
